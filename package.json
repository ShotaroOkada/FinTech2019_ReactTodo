{
  "name": "uim_todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-redux": "^7.1.0",
    "@types/redux-logger": "^3.0.7",
    "foreman": "^3.0.1",
    "immutability-helper": "^3.0.0",
    "immutable-helper": "^0.0.5",
    "react": "^16.11.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "tslib": "^1.10.0",
    "typesafe-actions": "^5.1.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.8.4",
    "typescript": "^3.6.4"
  },
  "resolutions": {
    "@types/react": "^16.9.11"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "productName": "UIM Todo",
    "appId": "try.example.com",
    "files": [
      "./src/electron-starter.js",
      "./icon/**/*",
      "*.js",
      "build",
      "node_modules"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "./src/images/umiIcon.png"
    },
    "win": {
      "target": "msi",
      "icon": "./src/images/umiIcon.png"
    },
    "linux": {
      "target": "deb",
      "icon": "./src/images/umiIcon.png",
      "maintainer": "Shotaro Okada <b1016117@gmail.com> (https://github.com/ShotaroOkada)"
    }
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron": "node_modules/.bin/electron .",
    "dev": "nf start -p 3000",
    "package:mac": "react-scripts-ts build && electron-builder --mac --x64",
    "package:windows": "react-scripts-ts build && electron-builder --win --x64",
    "package:linux": "react-scripts-ts build && electron-builder --linux --x64"
  },
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^22.1.0"
  }
}
