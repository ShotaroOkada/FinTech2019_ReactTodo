{
  "name": "umi_todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "^7.1.0",
    "@types/redux-logger": "^3.0.7",
    "foreman": "^3.0.1",
    "immutability-helper": "^3.0.0",
    "immutable-helper": "^0.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "productName": "UMI Todo",
    "appId": "try.example.com",
    "files": [
      "./src/electron-starter.js",
      "./icon/**/*",
      "*.js",
      "build",
      "node_modules"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": "msi",
      "icon": "./src/images/umiIcon.png"
    }
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron": "node_modules/.bin/electron .",
    "dev" : "nf start -p 3000",
    "package:mac": "react-scripts-ts build && electron-builder --mac --x64",
    "package:windows": "react-scripts-ts build && electron-builder --win --x64",
    "package:linux": "react-scripts-ts build && electron-builder --linux --x64"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "typescript": "^3.5.1",
    "electron": "^5.0.7"
  }
}
